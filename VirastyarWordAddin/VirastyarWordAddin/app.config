  <log4net>
    <!--<logger name="PersianWordAddin">
      <level value="ALL"/>
    </logger>-->
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file type="log4net.Util.PatternString" value="C:\PersianWordAddin\[%date{yyyy-MM-dd}]-Usage.log" />
      <appendToFile value="true" />
      <encoding value="unicodeFFFE" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date, [%thread], %-5level, %logger%newline%message%newline----------------------------%newline" />
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--<file value="c:\PersianWordAddin\usage.log" />-->
